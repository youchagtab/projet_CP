<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>projet_CP</display-name>
	<servlet>
		<servlet-name>cc</servlet-name>
		<servlet-class>controller.Acceuil</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>cc</servlet-name>
		<url-pattern>/acceuil</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>cd</servlet-name>
		<servlet-class>controller.Deconnexion</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>cd</servlet-name>
		<url-pattern>/deconnexion</url-pattern>
	</servlet-mapping>
	
</web-app>